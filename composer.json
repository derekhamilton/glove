{
    "name": "derekhamilton/glove",
    "description": "Exception handling for Laravel that catches like a glove.",
    "keywords": ["laravel", "exceptions", "exception", "handling"],
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "illuminate/contracts": "5.*|6.*",
        "illuminate/support": "5.*|6.*",
        "symfony/console": "^3.1|4.*",
        "symfony/debug": "^3.1|4.*",
        "symfony/http-foundation": "^3.1|4.*",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.1|7.*",
        "orchestra/testbench": "~3.0",
        "filp/whoops": "2.*"
    },
    "autoload": {
        "psr-4": {
            "DerekHamilton\\Glove\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DerekHamilton\\Tests\\Glove\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DerekHamilton\\Glove\\Providers\\GloveServiceProvider"
            ]
        }
    }
}
